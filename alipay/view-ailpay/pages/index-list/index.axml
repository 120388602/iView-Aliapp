<view class='page-container-classname'>
                <view class='i-index-demo'>
  <i-index height='100%' onChange='onChange'>
    <i-index-item class='i-index-item' a:for='{{ cities }}' a:for-index='index' a:key='*this' a:for-item='item' name='{{item.key}}'>
      <view class='i-index-demo-item' a:for='{{item.list}}' a:for-index='_in' a:key='{{_in}}' a:for-item='it'>
        {{it.name}}
      </view>
    </i-index-item>
  </i-index>
</view>
            </view>